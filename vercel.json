{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["nrt1"],
  "github": {
    "enabled": true,
    "silent": true,
    "autoAlias": false
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "preview": false
    }
  }
} 